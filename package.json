{
  "name": "@hhangular/hhangular",
  "version": "1.0.98",
  "author": "Achache francois",
  "license": "MIT",
  "description": "components for angular 8x by hhfrancois",
  "repository": {
    "type": "git",
    "url": "https://github.com/hhangular/hhangular.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "angular",
    "component"
  ],
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "start:website:fr-fr": "ng serve --base-href /fr-fr/",
    "start:website:en-us": "ng serve --port 4201 --configuration=en-us --base-href /en-us/",
    "build:website:fr-fr": "ng build --outputPath dist/website/fr-fr/ --base-href /fr-fr/",
    "build:website:en-us": "ng build --outputPath dist/website/en-us/ --i18n-file src/locale/messages.en-us.xlf --i18n-format xlf --i18n-locale en-us --base-href /en-us/",
    "build:website:prod:fr-fr": "ng build --outputPath dist/website/fr-fr/ --prod --base-href /fr-fr/",
    "build:website:prod:en-us": "ng build --outputPath dist/website/en-us/ --prod --i18n-file src/locale/messages.en-us.xlf --i18n-format xlf --i18n-locale en-us --base-href /en-us/",
    "build:websites:prod": "npm run build:website:prod:fr-fr && npm run build:website:prod:en-us",
    "test:website": "ng test hhangular",
    "test:website:ci": "ng test hhangular --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "watch:pdfjs": "ng build pdfjs --watch",
    "build:pdfjs:prod": "ng build pdfjs --prod",
    "test:pdfjs": "ng test pdfjs",
    "test:pdfjs:ci": "ng test pdfjs --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "watch:store": "ng build store --watch",
    "build:store:prod": "ng build store --prod",
    "test:store": "ng test store",
    "test:store:ci": "ng test store --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "watch:star-rating": "ng build star-rating --watch",
    "build:star-rating:prod": "ng build star-rating --prod",
    "test:star-rating": "ng test star-rating",
    "test:star-rating:ci": "ng test star-rating --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "extract-i18n": "ng xi18n --i18n-locale fr-fr --output-path dist/locale",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "copy:static": "node static.copy.js",
    "build:sitemap": "node sitemap.build.js",
    "serve:ssr": "node dist/server.js",
    "build:server:fr-fr": "ng run hhangular:server --outputPath dist/server/fr-fr",
    "build:server:en-us": "ng run hhangular:server --outputPath dist/server/en-us --i18n-file src/locale/messages.en-us.xlf --i18n-format xlf --i18n-locale en-us",
    "build:servers": "npm run build:server:fr-fr && npm run build:server:en-us",
    "build:client-and-server-bundles": "npm run build:websites:prod && npm run build:servers",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "heroku-postbuild": "npm run build:ssr && npm run copy:static && npm run build:sitemap",
    "docker:build": "docker build -t h2dev/hhangular .",
    "docker:run": "docker run -d --rm -p80:80 --name hhangular h2dev/hhangular"
  },
  "dependencies": {
    "@angular/animations": "8.2.13",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.13",
    "@angular/compiler": "8.2.13",
    "@angular/core": "8.2.13",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.13",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.13",
    "@angular/platform-browser-dynamic": "8.2.13",
    "@angular/platform-server": "8.2.13",
    "@angular/router": "8.2.13",
    "@fortawesome/angular-fontawesome": "0.14.1",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@hhangular/pdfjs": "2.0.39",
    "@hhangular/store": "1.0.15",
    "@hhangular/star-rating": "1.0.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@types/pdfjs-dist": "2.1.2",
    "compression": "1.7.4",
    "core-js": "3.3.6",
    "hammerjs": "2.0.8",
    "ngx-markdown": "8.2.1",
    "pdfjs-dist": "2.2.228",
    "prismjs": "1.17.1",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.17",
    "@angular-devkit/build-ng-packagr": "0.803.17",
    "@angular/cli": "8.3.17",
    "@angular/compiler-cli": "8.2.13",
    "@angular/language-service": "8.2.13",
    "@types/jasmine": "3.4.5",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "8.10.58",
    "codecov": "3.6.1",
    "codelyzer": "5.2.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "5.7.1",
    "protractor": "5.4.2",
    "ts-loader": "6.2.1",
    "ts-node": "8.4.1",
    "tsickle": "0.37.0",
    "tslint": "5.20.0",
    "typescript": "3.5.3",
    "webpack-cli": "3.3.10",
    "xmlbuilder": "13.0.2"
  }
}
