{
  "name": "@hhangular/hhangular",
  "version": "1.0.70",
  "author": "Achache francois",
  "license": "MIT",
  "description": "components for angular 8x by hhfrancois",
  "repository": {
    "type": "git",
    "url": "https://github.com/hhangular/hhangular.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "angular",
    "component"
  ],
  "scripts": {
    "ng": "ng",
    "start:website:fr-fr": "ng serve --verbose --base-href /fr-fr/",
    "start:website:en-us": "ng serve --port 4201 --configuration=en-us --base-href /en-us/",
    "build:website:fr-fr": "ng build --outputPath dist/website/fr-fr/ --base-href /fr-fr/",
    "build:website:en-us": "ng build --outputPath dist/website/en-us/ --i18n-file src/locale/messages.en-us.xlf --i18n-format xlf --i18n-locale en-us --base-href /en-us/",
    "build:website:prod:fr-fr": "ng build --outputPath dist/website/fr-fr/ --prod --base-href /fr-fr/",
    "build:website:prod:en-us": "ng build --outputPath dist/website/en-us/ --prod --i18n-file src/locale/messages.en-us.xlf --i18n-format xlf --i18n-locale en-us --base-href /en-us/",
    "test:website": "ng test hhangular",
    "test:website:ci": "ng test hhangular --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "build:pdfjs": "ng build pdfjs --watch",
    "build:pdfjs:prod": "ng build pdfjs --prod",
    "test:pdfjs": "ng test pdfjs",
    "test:pdfjs:ci": "ng test pdfjs --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "build:store": "ng build store --watch",
    "build:store:prod": "ng build store --prod",
    "test:store": "ng test store",
    "test:store:ci": "ng test store --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "extract-i18n": "ng xi18n --i18n-locale fr-fr --output-path dist/locale"
  },
  "dependencies": {
    "@angular/animations": "8.2.4",
    "@angular/cdk": "8.1.4",
    "@angular/common": "8.2.4",
    "@angular/compiler": "8.2.4",
    "@angular/core": "8.2.4",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.4",
    "@angular/material": "8.1.4",
    "@angular/platform-browser": "8.2.4",
    "@angular/platform-browser-dynamic": "8.2.4",
    "@angular/router": "8.2.4",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.22",
    "@fortawesome/free-brands-svg-icons": "5.10.2",
    "@fortawesome/free-regular-svg-icons": "5.10.2",
    "@fortawesome/free-solid-svg-icons": "5.10.2",
    "@types/pdfjs-dist": "2.1.0",
    "core-js": "3.2.1",
    "hammerjs": "2.0.8",
    "ngx-markdown": "8.1.0",
    "pdfjs-dist": "2.1.266",
    "prismjs": "1.17.1",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.2",
    "@angular-devkit/build-ng-packagr": "0.803.2",
    "@angular/cli": "8.3.2",
    "@angular/compiler-cli": "8.2.4",
    "@angular/language-service": "8.2.4",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "8.10.53",
    "codecov": "3.5.0",
    "codelyzer": "5.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "5.5.0",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tsickle": "0.37.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3"
  }
}
